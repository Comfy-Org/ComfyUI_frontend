{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    /* Build scripts configuration */
    "noEmit": true,
    "strict": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true
  },
  "include": [
    "**/*.ts"
  ]
}